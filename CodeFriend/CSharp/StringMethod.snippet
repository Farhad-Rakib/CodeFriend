<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2017/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Return Type String Method</Title>
			<Shortcut>mstrn</Shortcut>
			<Description>Code snippet for return type string method </Description>
			<Author>Farhad Rakib</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>

				<Literal>
					<ID>CreatedDate</ID>
					<ToolTip>Enter date of creation</ToolTip>
					<Default>[Insert Date]</Default>
				</Literal>

				<Literal>
					<ID>Developer</ID>
					<ToolTip>Enter your name</ToolTip>
					<Default>[My Name]</Default>
				</Literal>


				<Literal>
					<ID>Description</ID>
					<ToolTip>Enter your name</ToolTip>
					<Default>[Description of Class]</Default>
				</Literal>



				<Literal>
					<ID>MethodName</ID>
					<ToolTip>Give Your Method Name</ToolTip>
					<Default>Method</Default>
				</Literal>



			</Declarations>
			<Code Language="csharp">
				<![CDATA[
				 /*  
						 Created Date : $CreatedDate$  
						 Developer:    $Developer$
						 Description : $Description$
						 
				 */
				public string $MethodName$()
					{
					string returnString = string.Empty;
					
					return returnString;
					}]]>
			</Code>
		</Snippet>
	</CodeSnippet>


</CodeSnippets>

